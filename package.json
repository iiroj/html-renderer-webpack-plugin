  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@webpack-contrib/test-utils": "^0.1.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
