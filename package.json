{
  "name": "html-renderer-webpack-plugin",
  "version": "4.0.21",
  "description": "A webpack plugin for rendering static html pages",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/html-renderer-webpack-plugin",
  "repository": "github:iiroj/html-renderer-webpack-plugin",
  "bugs": "https://github.com/iiroj/html-renderer-webpack-plugin/issues",
  "main": "dist/HtmlRendererWebpackPlugin.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "watch": "rollup -cw",
    "build": "rollup -c",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm test && npm run build"
    }
  },
  "peerDependencies": {
    "webpack": ">=4"
  },
  "dependencies": {
    "@types/webpack-sources": "^0.1.5",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.0.0",
    "@types/enzyme": "^3.10.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/webpack": "^4.4.34",
    "@webpack-contrib/test-utils": "^0.1.3",
    "commitlint": "^8.0.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2",
    "rollup": "^1.16.6",
    "rollup-plugin-typescript2": "^0.21.2",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2"
  },
  "keywords": [
    "babel",
    "html",
    "renderer",
    "static-site",
    "static",
    "template",
    "webpack"
  ]
}
