{
  "name": "html-renderer-webpack-plugin",
  "version": "5.3.0",
  "description": "A webpack plugin for rendering static html pages",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/html-renderer-webpack-plugin",
  "repository": "github:iiroj/html-renderer-webpack-plugin",
  "bugs": "https://github.com/iiroj/html-renderer-webpack-plugin/issues",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "lint:base": "eslint --ext .js,.ts",
    "lint": "npm run lint:base -- .",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.test.json --noEmit",
    "test": "jest",
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:base --"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run typecheck && npm test && npm run build"
    }
  },
  "peerDependencies": {
    "webpack": ">=4"
  },
  "dependencies": {
    "@types/webpack-sources": "^2.0.0",
    "chokidar": "^3.4.3",
    "webpack-sources": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-typescript": "6.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/webpack": "^4.41.24",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rollup": "2.33.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^4.35.3"
  },
  "keywords": [
    "babel",
    "html",
    "renderer",
    "static-site",
    "static",
    "template",
    "webpack"
  ]
}
